{
  "name": "node-typescript-api",
  "version": "0.0.1",
  "author": {
    "name": "Michal Jarnot",
    "email": "m.jarnot@yahoo.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf build && node_modules/.bin/babel src --out-dir build --copy-files --extensions '.js,.ts'",
    "dev": "node_modules/.bin/nodemon -L --exec \"node_modules/.bin/babel-node --extensions '.js,.ts' src/index.ts\"",
    "db:create:migration": "node_modules/.bin/knex migrate:make --knexfile=./knexfile.ts",
    "db:migrate": "node_modules/.bin/knex migrate:latest --knexfile=./knexfile.ts",
    "db:rollback": "node_modules/.bin/knex migrate:rollback --knexfile=./knexfile.ts",
    "db:seed": "node_modules/.bin/knex seed:run --knexfile=./knexfile.ts",
    "test": "node_modules/.bin/jest -i --forceExit --detectOpenHandles",
    "test:coverage": "node_modules/.bin/jest -i --forceExit --detectOpenHandles --coverage",
    "heroku-postbuild": "npm run build && npm run db:migrate",
    "prod": "node build",
    "lint:ts": "tslint './src/**/*.{ts,tsx}'"
  },
  "type-check": "tsc",
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write --loglevel warn",
      "tslint --force --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "6.4.0",
    "bcrypt": "3.0.6",
    "boom": "7.3.0",
    "dotenv": "8.0.0",
    "faker": "^4.1.0",
    "ip": "1.1.5",
    "jsonschema": "1.2.4",
    "jsonwebtoken": "8.5.1",
    "kcors": "2.2.2",
    "knex": "0.16.5",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-compose": "4.1.0",
    "koa-mount": "4.0.0",
    "koa-router": "7.4.0",
    "koa-static": "5.0.0",
    "moment": "2.24.0",
    "nanoid": "2.0.3",
    "objection": "1.6.9",
    "pg": "7.11.0",
    "pino": "5.12.6",
    "ramda": "0.26.1",
    "redoc": "2.0.0-rc.4",
    "slugify": "1.3.4",
    "superagent": "5.0.5",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/node": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@types/bcrypt": "3.0.0",
    "@types/boom": "7.2.1",
    "@types/dotenv": "6.1.1",
    "@types/faker": "^4.1.5",
    "@types/ip": "1.1.0",
    "@types/jest": "24.0.13",
    "@types/jsonschema": "1.1.1",
    "@types/jsonwebtoken": "8.3.2",
    "@types/kcors": "2.2.3",
    "@types/knex": "0.16.1",
    "@types/koa": "2.0.48",
    "@types/koa-bodyparser": "5.0.2",
    "@types/koa-compose": "3.2.4",
    "@types/koa-mount": "3.0.1",
    "@types/koa-router": "7.0.40",
    "@types/koa-static": "4.0.1",
    "@types/moment": "2.13.0",
    "@types/nanoid": "1.2.1",
    "@types/node": "12.0.3",
    "@types/pino": "5.8.13",
    "@types/ramda": "0.26.9",
    "@types/sendgrid": "4.3.0",
    "@types/sinon": "7.0.12",
    "@types/superagent": "4.1.1",
    "@types/supertest": "2.0.7",
    "@types/twilio": "0.0.10",
    "@types/uuid": "3.4.4",
    "babel-plugin-module-resolver": "3.2.0",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "lint-staged": "8.1.7",
    "nodemon": "1.19.1",
    "prettier": "1.17.0",
    "sinon": "7.3.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.1",
